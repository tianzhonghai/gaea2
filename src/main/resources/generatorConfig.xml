            <!--<columnOverride column="role_sign" property="roleSign"></columnOverride>-->
            <!--<columnOverride column="description" property="description"></columnOverride>-->
        <!--</table>-->

        <table tableName="user_role" domainObjectName="UserRolePO">
            <property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>
            <generatedKey column="id" sqlStatement="MySql" identity="true"></generatedKey>
            <columnOverride column="user_id" property="userId"></columnOverride>
            <columnOverride column="role_id" property="roleId"></columnOverride>
        </table>

        <table tableName="permission" domainObjectName="PermissionPO">
            <property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>
            <generatedKey column="id" sqlStatement="MySql" identity="true"></generatedKey>
            <columnOverride column="permission_name" property="permissionName"></columnOverride>
            <columnOverride column="permission_sign" property="permissionSign"></columnOverride>
            <columnOverride column="description" property="description"></columnOverride>
        </table>

        <table tableName="role_permission" domainObjectName="RolePermissionPO">
            <property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>
            <generatedKey column="id" sqlStatement="MySql" identity="true"></generatedKey>
            <columnOverride column="permission_id" property="permissionId"></columnOverride>
            <columnOverride column="role_id" property="roleId"></columnOverride>
        </table>
    </context>
